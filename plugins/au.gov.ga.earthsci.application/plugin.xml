<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension-point id="catalogTreeLabelProvider" name="Catalog Tree Label Provider" schema="schema/au.gov.ga.earthsci.catalog.CatalogTreeLabelProvider.exsd"/>
   <extension-point id="bookmarkPropertyEditor" name="Bookmark Property Editor" schema="schema/au.gov.ga.earthsci.bookmark.editor.exsd"/>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="au.gov.ga.earthsci.application"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="appName"
               value="au.gov.ga.earthsci.application">
         </property>
         <property
               name="applicationXMI"
               value="au.gov.ga.earthsci.application/Application.e4xmi">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/au.gov.ga.earthsci.application/css/default.css">
         </property>
         <property
         	name="lifeCycleURI"
         	value="bundleclass://au.gov.ga.earthsci.application/au.gov.ga.earthsci.application.LifeCycleManager">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="au.gov.ga.earthsci.notification.preferences.NotificationPreferencesPage"
            id="au.gov.ga.earthsci.notifications"
            name="Notifications">
      </page>
      <page
            class="au.gov.ga.earthsci.catalog.part.preferences.CatalogBrowserPreferencesPage"
            id="au.gov.ga.earthsci.catalogs"
            name="Catalogs">
      </page>
      <page
            class="au.gov.ga.earthsci.bookmark.part.preferences.BookmarksPreferencesPage"
            id="au.gov.ga.earthsci.bookmarks"
            name="Bookmarks">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="au.gov.ga.earthsci.catalog.part.preferences.CatalogBrowserPreferenceInitializer">
      </initializer>
      <initializer
            class="au.gov.ga.earthsci.bookmark.part.preferences.BookmarksPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="au.gov.ga.earthsci.notification.receiver">
      <receiver
            class="au.gov.ga.earthsci.notification.dialog.DialogNotificationReceiver">
      </receiver>
      <receiver
            class="au.gov.ga.earthsci.notification.part.NotificationPartReceiver">
      </receiver>
   </extension>
   <extension
         point="au.gov.ga.earthsci.application.catalogTreeLabelProvider">
      <provider
            class="au.gov.ga.earthsci.catalog.part.DatasetCatalogTreeLabelProvider">
      </provider>
   </extension>
   <extension
         point="au.gov.ga.earthsci.injectable">
      <bind
            binding="au.gov.ga.earthsci.catalog.part.ICatalogBrowserController"
            implementation="au.gov.ga.earthsci.catalog.part.CatalogBrowserController">
      </bind>
      <bind
            binding="au.gov.ga.earthsci.catalog.part.preferences.ICatalogBrowserPreferences"
            implementation="au.gov.ga.earthsci.catalog.part.preferences.CatalogBrowserPreferences">
      </bind>
      <bind
            binding="au.gov.ga.earthsci.bookmark.part.IBookmarksController"
            implementation="au.gov.ga.earthsci.bookmark.part.BookmarksController">
      </bind>
      <bind
            binding="au.gov.ga.earthsci.bookmark.part.preferences.IBookmarksPreferences"
            implementation="au.gov.ga.earthsci.bookmark.part.preferences.BookmarksPreferences">
      </bind>
      <injectable
            class="au.gov.ga.earthsci.catalog.part.CatalogTreeLabelProviderRegistry">
      </injectable>
      <injectable
            class="au.gov.ga.earthsci.bookmark.part.editor.BookmarkPropertyEditorFactory">
      </injectable>
   </extension>
   <extension
         point="au.gov.ga.earthsci.application.bookmarkPropertyEditor">
      <editor
            class="au.gov.ga.earthsci.bookmark.part.editor.CameraPropertyEditor"
            type="au.gov.ga.earthsci.bookmark.properties.camera">
      </editor>
      <editor
            class="au.gov.ga.earthsci.bookmark.part.editor.LayersPropertyEditor"
            type="au.gov.ga.earthsci.bookmark.properties.layers">
      </editor>
   </extension>

</plugin>
