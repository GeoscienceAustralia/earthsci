<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="au.gov.ga.earthsci.intent.dispatchFilters">
      <filter
            class="au.gov.ga.earthsci.layer.ui.LayerTreeNodeDispatchHandler"
            name="Layer"
            priority="0">
         <type
               class="au.gov.ga.earthsci.layer.tree.ILayerTreeNode">
         </type>
         <type
               class="gov.nasa.worldwind.layers.Layer">
         </type>
      </filter>
   </extension>
   <extension
         point="au.gov.ga.earthsci.editable.models">
      <model
            model="au.gov.ga.earthsci.layer.ui.edit.LayerModelElement"
            sdef-context="au.gov.ga.earthsci.layer.ui.edit.LayerModelElement"
            sdef-name="Layer"
            type="gov.nasa.worldwind.layers.Layer">
      </model>
      <model
            model="au.gov.ga.earthsci.layer.ui.edit.SkyGradientLayerWrapperModelElement"
            sdef-context="au.gov.ga.earthsci.layer.ui.edit.SkyGradientLayerWrapperModelElement"
            sdef-name="SkyGradientLayerWrapper"
            type="au.gov.ga.earthsci.layer.wrappers.SkyGradientLayerWrapper">
      </model>
      <model
            model="au.gov.ga.earthsci.layer.ui.edit.TiledImageLayerModelElement"
            sdef-context="au.gov.ga.earthsci.layer.ui.edit.TiledImageLayerModelElement"
            sdef-name="TiledImageLayer"
            type="gov.nasa.worldwind.layers.TiledImageLayer">
      </model>
      <model
            model="au.gov.ga.earthsci.layer.ui.edit.TiledImageLayerWrapperModelElement"
            sdef-context="au.gov.ga.earthsci.layer.ui.edit.TiledImageLayerWrapperModelElement"
            sdef-name="TiledImageLayerWrapper"
            type="au.gov.ga.earthsci.layer.wrappers.TiledImageLayerWrapper">
      </model>
   </extension>

</plugin>
