<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="au.gov.ga.earthsci.core.xmlLoaders">
      <loader
            class="au.gov.ga.earthsci.catalog.wms.WMSCapabilitiesXmlLoader"
            filter="au.gov.ga.earthsci.catalog.wms.WMSCapabilitiesXmlLoader"
            priority="0">
      </loader>
   </extension>
   <extension
         point="au.gov.ga.earthsci.core.mimeTypes">
      <mime-type
            content-type-id="org.eclipse.core.runtime.xml"
            name="application/vnd.ogc.wms_xml">
      </mime-type>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            id="au.gov.ga.earthsci.catalog.wms.wmslayer"
            name="WMS layer"
            priority="normal">
      </content-type>
   </extension>
   <extension
         point="au.gov.ga.earthsci.intent.filters">
      <filter
            class="au.gov.ga.earthsci.catalog.wms.WMSLayerIntentHandler"
            priority="0">
         <content-type
               id="au.gov.ga.earthsci.catalog.wms.wmslayer">
         </content-type>
         <return-type
               class="gov.nasa.worldwind.layers.Layer">
         </return-type>
         <uri
               scheme="wmslayer">
         </uri>
      </filter>
   </extension>

</plugin>
